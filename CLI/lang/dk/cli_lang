	declare text="${quickbox_lock} kører i øjeblikket
processen vil blive genoptaget, når ${quickbox_lock} er færdig"
	quickbox::dashboard::log "${text}"
}

quickbox::lang::lock::remove() {
	declare text="fjernelse af softwarelåse..."
	quickbox::dashboard::log "${text}"
}

